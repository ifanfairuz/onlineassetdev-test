{
  "name": "onlineassetdev-test",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "imports": {
    "#frontend": {
      "development": "./src/frontend.dev.js",
      "default": "./dist/frontend.js"
    }
  },
  "scripts": {
    "dev": "tsx watch -C development src/main.ts",
    "build": "run-p build:client build:ssr && run-p build:server",
    "build:client": "vite build --outDir dist/public --ssrManifest",
    "build:ssr": "vite build --ssr src/frontend/main.server.ts --outDir dist/ssr",
    "build:server": "tsc -p tsconfig.build.json --outDir dist",
    "db:migrate": "tsx src/api/database/migrate.ts",
    "db:migrate:reset": "tsx src/api/database/migrate.ts --drop",
    "db:seed": "tsx src/api/database/seed.ts",
    "start": "node dist/main.js",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "pinia": "^3.0.4",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.17",
    "@tsconfig/node22": "^22.0.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/node": "^22.18.11",
    "@types/pg": "^8.15.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
